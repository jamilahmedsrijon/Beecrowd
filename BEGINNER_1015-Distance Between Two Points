#include<stdio.h>
#include<math.h>

int main()
{   double x1, x2, y1, y2;
    scanf("%lf%lf",&x1,&y1);
    scanf("%lf%lf",&x2,&y2);
    double x = (x2 - x1);
    double x3 = x * x;
    double y = (y2 - y1);
    double y3 = y * y;
    double Distance = sqrt( x3 + y3);
    printf("%.4lf\n",Distance);


    return 0;
}
